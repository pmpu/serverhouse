@using serverhouse_web.PropertyValueRepresentation
@model serverhouse_web.Models.SHObject.SHObject


@{ var obj = Model; }


<div class="obj_view_article">

    @foreach (var prop in obj.properties.Where(o => o.Key != "name")
        .OrderBy(o => o.Value.order_data["row"])) {
        <h3>@prop.Key</h3>

        @Html.Partial(PropertyValueRepresentation.getViewPartialViewName(prop.Value.type), prop.Value)
        ;
    }
</div>